  :src=\"detailProduk.gambar_utama\"\r\n                    \r\n                    eager\r\n                    class=\"white--text align-end\"\r\n                  />\r\n                </v-carousel-item>\r\n              </v-carousel>\r\n            </v-card>\r\n          </div>\r\n          <!-- deskripsi produk dan spesifikasi -->\r\n          <div class=\"col-9\">\r\n            <v-card class=\"mx-auto\" max-width=\"100%\" height=\"auto\">\r\n              <v-card-text>\r\n                <p class=\"text-h4 text--primary\">{{ detailProduk.nama }}</p>\r\n                <!-- mengambil input -->\r\n                <!-- <v-text-field\r\n                  placeholder=\"0\"\r\n                  filled\r\n                  rounded\r\n                  dense\r\n                  v-model=\"cart.nama\"\r\n                  hidden\r\n                ></v-text-field>\r\n                <v-text-field\r\n                  placeholder=\"0\"\r\n                  filled\r\n                  rounded\r\n                  dense\r\n                  v-model=\"cart.harga\"\r\n                  hidden\r\n                ></v-text-field> -->\r\n                <p class=\"text-h5 text--primary\">{{detailProduk.harga}}</p>\r\n                <!-- DESKRIPSI SINGKAT -->\r\n                <div class=\"text--primary\">\r\n                  {{detailProduk.deskripsi}}\r\n                </div>\r\n              </v-card-text>\r\n              <v-card-actions>\r\n                <v-btn text color=\"teal accent-4\" @click=\"reveal = true\">\r\n                  Lihat spesifikasi\r\n                </v-btn>\r\n              </v-card-actions>\r\n              <!-- jumlah pesanan -->\r\n              <v-col cols=\"12\" sm=\"6\" md=\"4\">\r\n                <p>stock tersedia : {{detailProduk.stock}}</p>\r\n                <v-text-field\r\n                  placeholder=\"0\"\r\n                  filled\r\n                  rounded\r\n                  dense\r\n                  v-model=\"cart.jumlah\"\r\n                ></v-text-field>\r\n                <v-card-actions class=\"pt-0\">\r\n                    <v-btn text color=\"teal accent-4\" v-on:click=\"submit()\"><v-icon>mdi-cart</v-icon>\r\n                       Add to cart\r\n                    </v-btn>\r\n                  </v-card-actions>\r\n              </v-col>\r\n              <v-expand-transition>\r\n                <v-card\r\n                  v-if=\"reveal\"\r\n                  class=\"transition-fast-in-fast-out v-card--reveal\"\r\n                  style=\"height: 100%\"\r\n                >\r\n                  <v-card-text class=\"pb-0\">\r\n                    <p class=\"text-h4 text--primary\">\r\n                      {{detailProduk.nama}}\r\n                    </p>\r\n                  <p class=\"text-h5 text--primary\">{{detailProduk.harga}}</p>\r\n                  <div class=\"row\">\r\n                            {{detailProduk.spesifikasi}}\r\n                  </div>\r\n                  </v-card-text>\r\n                  <v-card-actions class=\"pt-0\">\r\n                    <v-btn text color=\"teal accent-4\" @click=\"reveal = false\">\r\n                      Close\r\n                    </v-btn>\r\n                  </v-card-actions>\r\n                </v-card>\r\n              </v-expand-transition>\r\n            </v-card>\r\n          </div>\r\n        </div>\r\n      </v-app>\r\n    </div>\r\n    <Footer />\r\n  </v-app>\r\n</template>\r\n<script>\r\nimport Navbar from \"../components/Navbar\";\r\nimport Footer from \"../components/Footer\";\r\nimport Vue from 'vue'\r\n import axios from 'axios'\r\n import VueAxios from 'vue-axios'\r\n Vue.use(VueAxios, axios)\r\n\r\nexport default {\r\n  name: \"detail\",\r\n\r\n  components: {\r\n    Navbar,\r\n    Footer\r\n  },\r\n  data() {\r\n    return {\r\n      slides: [\r\n        {\r\n          src: require(\"../assets/image-produk/oppo_a96.jpg\"),\r\n        },\r\n        {\r\n          src: require(\"../assets/image-produk/samsung_galaxy_m53.jpg\"),\r\n        },\r\n        {\r\n          src: require(\"../assets/icon/vivo.png\"),\r\n        },\r\n      ],\r\n      reveal: false,\r\n      detailProduk: [],\r\n      pesan: {\r\n                nama: '',\r\n                pelanggan_id: this.$route.params.id,\r\n                harga: '',\r\n                jumlah: '',\r\n                total_harga: ''\r\n            }\r\n    };\r\n\r\n  },\r\n  mounted(){\r\n    this.getProduk()\r\n  },\r\n  methods: {\r\n    getProduk(){\r\n      Vue.axios.get('http://localhost/galaxyphone/api/produk/detail.php?id='+this.$route.params.id)\r\n      .then((response) =>{\r\n        this.detailProduk = response.data\r\n        console.log(this.detailProduk)\r\n      }\r\n      )\r\n    },\r\n    // submit(){\r\n    //         Vue.axios.post('http://localhost/galaxyphone/api/keranjang/create.php', this.pesan)\r\n    //             .then(function( response ){\r\n    //                 this.$toast.success(\"Sukses Masuk Keranjang\", {\r\n    //           type: \"success\",\r\n    //           position: \"top-right\",\r\n    //           duration: 3000,\r\n    //           dismissible: true,\r\n    //         });\r\n    //             }.bind(this));\r\n    //     }\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.v-card--reveal {\r\n  bottom: 0;\r\n  opacity: 1 !important;\r\n  position: absolute;\r\n  width: 100%;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}