           </v-card>\r\n            </div>\r\n            <div class=\"col-10\">\r\n              <!-- card -->\r\n              <v-content>\r\n                <v-container fluid grid-list-md>\r\n                  <v-layout row wrap>\r\n                    <v-flex class=\"mx-auto\" v-for=\"n in listProduk\" :key=\"n.id\">\r\n                      <v-card class=\"ma-3 text-center\" height=\"410\" width=\"250\">\r\n                        <!-- batas -->\r\n                        <row>\r\n                          <v-img\r\n                            class=\"white--text align-end\"\r\n                            width=\"100%\"\r\n                            :src=\"n.gambar_utama_produk\"\r\n                          >\r\n                          </v-img>\r\n                        </row>\r\n\r\n                        <v-card-subtitle class=\"pb-0 font-weight-black\">{{\r\n                          n.nama_produk\r\n                        }}</v-card-subtitle>\r\n\r\n                        <v-card-text class=\"text--primary\">\r\n                          <div>Rp. {{ n.harga_produk }}</div>\r\n                        </v-card-text>\r\n\r\n                        <div class=\"row\">\r\n                          <div class=\"col-6 text-center ma-auto\">\r\n                            <p>Terjual : {{ n.hasil_penjualan_produk }}</p>\r\n                          </div>\r\n                          <div class=\"col-6 d-flex justify-md-end\">\r\n                            <v-card-actions class=\"d-flex justify-lg-end\">\r\n                              <v-btn color=\"orange\" text>\r\n                                <router-link\r\n                                  class=\"text-decoration-none\"\r\n                                  :to=\"'/detail/' + n.produk_id\"\r\n                                  ><v-icon>mdi-eye</v-icon> Lihat</router-link\r\n                                >\r\n                              </v-btn>\r\n                            </v-card-actions>\r\n                          </div>\r\n                        </div>\r\n                        <v-row\r\n                          class=\"fill-height\"\r\n                          align=\"center\"\r\n                          justify=\"center\"\r\n                        >\r\n                          <v-scale-transition>\r\n                            <v-icon\r\n                              v-if=\"active\"\r\n                              color=\"white\"\r\n                              size=\"48\"\r\n                              v-text=\"'mdi-close-circle-outline'\"\r\n                            ></v-icon>\r\n                          </v-scale-transition>\r\n                        </v-row>\r\n                        <!-- batas -->\r\n                      </v-card>\r\n                    </v-flex>\r\n                  </v-layout>\r\n                </v-container>\r\n              </v-content>\r\n              <!-- card -->\r\n            </div>\r\n          </div>\r\n        </v-container>\r\n      </v-app>\r\n    </div>\r\n    <Footer />\r\n  </v-app>\r\n</template>\r\n<script>\r\nimport Navbar from \"../components/Navbar\";\r\nimport Footer from \"../components/Footer\";\r\nimport Vue from \"vue\";\r\nimport axios from \"axios\";\r\nimport VueAxios from \"vue-axios\";\r\nVue.use(VueAxios, axios);\r\nexport default {\r\n  name: \"ListKategori\",\r\n\r\n  components: {\r\n    Navbar,\r\n    Footer,\r\n  },\r\n  data() {\r\n    return {\r\n      items: [\r\n        {\r\n          text: \"Samsung\",\r\n          src: \"/listkategori/\" + 1,\r\n          title: 'coba',\r\n        },\r\n        {\r\n          text: \"SONY\",\r\n          src: \"/listkategori/\" + 11,\r\n        },\r\n        {\r\n          text: \"VIVO\",\r\n          src: \"/listkategori/\" + 7,\r\n        },\r\n        {\r\n          text: \"Xiaomi\",\r\n          src: \"/listkategori/\" + 2,\r\n        },\r\n        {\r\n          text: \"OPPO\",\r\n          src: \"/listkategori/\" + 4,\r\n        },\r\n        {\r\n          text: \"Iphone\",\r\n          src: \"/listkategori/\" + 12,\r\n        },\r\n      ],\r\n      listProduk: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    Vue.axios\r\n      .get(\r\n        \"http://localhost/galaxyphone/api/kategori_produk/list.php?id=\" +\r\n          this.$route.params.id\r\n      )\r\n      .then((response) => {\r\n        this.listProduk = response.data;\r\n        console.log(this.listProduk);\r\n      });\r\n  },\r\n  methods: {\r\n    refresh() {\r\n      location.reload();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}